<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="20191125225100" author="kobilovm">
        <createTable tableName="target">
            <column name="track_number" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>

            <column name="time" type="timestamp"/>
            <column name="lat" type="double precision"/>
            <column name="lon" type="double precision"/>
            <column name="x" type="double precision"/>
            <column name="y" type="double precision"/>
            <column name="vx" type="double precision"/>
            <column name="vy" type="double precision"/>
            <column name="heading" type="double precision"/>
            <column name="speed" type="double precision"/>
            <column name="status" type="boolean"/>


        </createTable>
    </changeSet>
</databaseChangeLog>
